<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.erp.distribution">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sfa.master.syncronize_fromserver.SyncronizeActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".sfa.transaksi.sales_order.SalesOrderActivity" />
        <activity android:name=".sfa.transaksi.sales_order.SalesOrderFormActivity" />
        <activity android:name=".sfa.master.customer_ref.CustomerRefActivity" />
        <activity android:name=".sfa.master.Customer.CustomerActivity" />
        <activity android:name=".sfa.master.material_note.NoteActivity" />
        <activity android:name=".sfa.master.material.MaterialActivity" />
    </application>

</manifest>